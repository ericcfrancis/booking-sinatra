<div class="container">
    <% @users.each do |user| %>
    <h1>Welcome <%= user.role ? "admin" : "user" %> <%= user.username %></h1>
    <div class="col-lg-8">
    </div>
        <p><a href="/edit/<%= user.id %>">edit</a></p>
        <p><a href="/delete/<%= user.id %>" data-method="delete">delete</a></p>
        <p><a href="/logout">logout</a></p>
    <% end %>
    
    <% @rooms.each do |room| %>
    <a href="/book_room/<%= room.id %>">
        <div class="col-lg-8 card">
            <ul>
                <li><%= room.room_name %></li>
                <li>RM<%= room.rate %></li>
                <li><%= room.capacity %> people</li>
            </ul>
        </div>
    </a>
    
    <% end %>
</div>
